{
  "name": "itkwidgets",
  "version": "0.32.0",
  "homepage": "https://github.com/InsightSoftwareConsortium/itkwidgets",
  "description": "Interactive Jupyter widgets to visualize images in 2D and 3D",
  "author": {
    "name": "Matthew McCormick",
    "email": "matt.mccormick@kitware.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/InsightSoftwareConsortium/itkwidgets/issues"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/InsightSoftwareConsortium/itkwidgets.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "itk",
    "imaging",
    "visualization",
    "webgl",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "dist/itk/**",
    "style/index.js"
  ],
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf itkwidgets-labextension/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jlpm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@jupyterlab/builder": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-loader": "^8.1.0",
    "babel-preset-mobx": "^2.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.1.1",
    "raw-loader": "^1.0.0",
    "rimraf": "^3.0.2",
    "shader-loader": "^1.3.1",
    "style-loader": "^0.23.1",
    "typescript": "~4.1.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^4.0.0",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3",
    "autoprefixer": "^9.7.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-element-queries": "^1.2.3",
    "itk": "^13.1.3",
    "itk-vtk-viewer": "^10.7.4",
    "jupyter-dataserializers": "^2.2.0",
    "mobx": "^5.15.4",
    "regenerator-runtime": "^0.13.5",
    "vtk.js": "^14.12.0"
  },
  "jupyterlab": {
    "extension": "dist/labextension",
    "outputDir": "itkwidgets-labextension/labextension"
  },
  "styleModule": "style/index.js"
}
